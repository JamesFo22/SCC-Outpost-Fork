<section class="version-history">
  <h2>Change history</h3>
  <% if false %>
  
  <% if @versions.present? %>
    <ul class="stepper">
        <% @versions.each do |v| %>
            <li class="stepper__step <%= "stepper__step--solid" if v.event === "create" %>">
                <p><%= v.event.humanize %>d by <strong><%= User.find(v.whodunnit).email %></strong></p>
                <p class="stepper__time"><%= time_ago_in_words(v.created_at).humanize %> ago</p>
            </li class="stepper__step">
        <% end %>
    <ul>
  <% else %>
    <p>Created <%= time_ago_in_words(@service.created_at) %> ago</p>
  <% end %>

  <% end %>
</section>