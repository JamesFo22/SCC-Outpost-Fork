<datalist id="link-label-options">
    <option value="Facebook">
    <option value="Twitter">
    <option value="Instagram">
    <option value="LinkedIn">
    <option value="YouTube">
</datalist>

<%= mark_unapproved_array("links") do %>

    <section class="repeater">
        <ul class="repeater__panels" aria-live="polite">
            <%= s.fields_for :links do |c| %>
                <li class="repeater__panel">
                    <%= render "admin/services/editors/links-fields", c: c %>
                </li>
            <% end %>
        </ul>
        <%= link_to_add_fields "Add a link", s, :links, "admin/services/editors/links-fields" %>
    </section>
<% end %>